#include <iostream>

using namespace std;

class C {
   public:
      double length;   
      double breadth;  
      double height;   
};

int main()
{
   C C1, C2;
   double volume = 0.0;    
 
   C1.height = 6.0; 
   C1.length = 8.0; 
   C1.breadth = 7.0;

   C2.height = 17.0;
   C2.length = 18.0;
   C2.breadth = 16.0;
   
   volume = C1.height * C1.length * C1.breadth;
   cout << "Volume of Box1 : " << volume <<endl;

   volume = C2.height * C2.length * C2.breadth;
   cout << "Volume of Box2 : " << volume <<endl;
   
   return 0;
}
