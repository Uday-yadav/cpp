#include <iostream>
using namespace std;
class O
{
    public:
    int sum(int a, int b)
    {
        return (a/b);
    }
    float sum(double a, double b)
    {
        return (a/b);
    }
    int sum(int a, int b, int c)
    {
        return ((a/b)/c);
    }
};
int main(void)
{
    O o;
    cout <<o.sum(4,4) <<"\n";
    cout <<o.sum(5.5,7.6) <<"\n";
    cout <<o.sum(4,7,9) <<"\n";
    
    return 0;
}

