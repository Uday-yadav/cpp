#include <iostream>
using namespace std;
 
int main()
{
    int a,b,add,sub,mul,divd;
    cout <<"Enter number 1: \n";
    cin>>a;
    cout<<"Enter number 2: \n";
    cin>>b;
    add=a+b;
    sub=a-b;
    mul=a*b;
    divd=a/b;
    cout<<"Add:" << add << endl;
    cout<<"Subtract:" << sub << endl;
    cout<<"Multiply:" << mul << endl;
    cout<<"Divide:" << divd << endl;
    return 0;
}
